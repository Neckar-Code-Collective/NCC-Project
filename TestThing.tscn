# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	if not is_multiplayer_authority():
		print(\"starting timer\")
		var timer = Timer.new()
		timer.timeout.connect(on_timer)
		add_child(timer)
		timer.start(5)

func on_timer():
	rpc_kill.rpc()

@rpc(\"any_peer\")
func rpc_kill():
	print(\"calling kill on \",multiplayer.get_unique_id())
	if not is_multiplayer_authority():
		return
	queue_free()

